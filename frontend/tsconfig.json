{
  "references": [
    {
      "path": "../shared"
    }
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "useDefineForClassFields": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noEmit": true,
    "isolatedModules": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
    "rootDir": "src/",
    "paths": {
      "#shared/*": [
        "../../shared/src/*"
      ],
      "#types/*": [
        "../../shared/src/types/*"
      ],
      "#components/*": [
        "./components/*"
      ],
      "#pages/*": [
        "./pages/*"
      ],
      "#assets/*": [
        "./assets/*"
      ],
      "#providers/*": [
        "./providers/*"
      ],
    }
  }
}